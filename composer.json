{
    "name": "happyr/auth0-bundle",
    "type": "symfony-bundle",
    "description": "Symfony integration with auth0",
    "license": "MIT",
    "authors": [
        {
            "name": "Tobias Nyholm",
            "email": "tobias.nyholm@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "psr/log": "~1.0",
        "psr/cache": "~1.0",
        "symfony/framework-bundle": "^3.4 || ^4.2",
        "symfony/security-bundle": "^3.4 || ^4.2",
        "php-http/httplug": "^1.0 || ^2.0",
        "php-http/client-common": "^1.9 || ^2.0",
        "php-http/discovery": "^1.6.1",
        "php-http/message": "^1.7.2",
        "php-http/cache-plugin": "^1.3",
        "auth0/auth0-php": "6.0.0-alpha.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "matthiasnoback/symfony-dependency-injection-test": "^3.1 || ^4.0",
        "nyholm/symfony-bundle-test": "^1.4",
        "php-http/message-factory": "^1.0.2",
        "php-http/curl-client": "^1.6",
        "php-http/mock-client": "^1.0",
        "nyholm/psr7": "^1.0"
    },
    "autoload": {
        "psr-4": { "Happyr\\Auth0Bundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-ci": "vendor/bin/phpunit --coverage-text --coverage-clover=build/coverage.xml"
    }
}
