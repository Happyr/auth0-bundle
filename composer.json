{
    "name": "happyr/auth0-bundle",
    "type": "symfony-bundle",
    "description": "Symfony integration with auth0",
    "license": "MIT",
    "authors": [
        {
            "name": "Tobias Nyholm",
            "email": "tobias.nyholm@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "auth0/auth0-php": "6.0.0-alpha.2",
        "php-http/cache-plugin": "^1.7",
        "php-http/client-common": "^1.10 || ^2.3",
        "php-http/discovery": "^1.13",
        "php-http/httplug": "^1.0 || ^2.0",
        "php-http/message": "^1.10",
        "psr/cache": "^1.0",
        "psr/log": "^1.0",
        "symfony/config": "^5.2",
        "symfony/framework-bundle": "^5.2",
        "symfony/security-bundle": "^5.2",
        "symfony/security-core": "^5.2"
    },
    "require-dev": {
        "nyholm/psr7": "^1.1",
        "nyholm/symfony-bundle-test": "^1.7",
        "php-http/message-factory": "^1.0.2",
        "php-http/mock-client": "^1.4",
        "symfony/phpunit-bridge": "^5.2"
    },
    "suggest": {
        "symfony/proxy-manager-bridge": "To make sure services are lazy."
    },
    "config": {
        "platform": {
            "php": "7.4.8"
        }
    },
    "autoload": {
        "psr-4": {
            "Happyr\\Auth0Bundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Happyr\\Auth0Bundle\\Tests\\": "tests"
        }
    }
}
